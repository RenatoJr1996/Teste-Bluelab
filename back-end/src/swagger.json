                        "schema": {
                            "type": "object",
                            "properties": {
                                
                            },
                            "example": {
                                
                            }
                        }
                    }
                }
            },
            "responses": {
                "201": {
                    "description": "Array of users"
                   
                }
            }
        }
    }
    }
